		"@types/node": "^17.0.21",
		"@types/node-cron": "^3.0.1",
		"@types/react": "^17.0.39",
		"@types/react-dom": "^17.0.13",
		"@typescript-eslint/eslint-plugin": "^5.13.0",
		"@typescript-eslint/parser": "^5.13.0",
		"autoprefixer": "^10.4.2",
		"babel-eslint": "^10.1.0",
		"babel-loader": "^8.2.3",
		"concurrently": "^7.0.0",
		"css-loader": "^6.7.0",
		"eslint": "^8.10.0",
		"eslint-config-google": "^0.14.0",
		"eslint-plugin-react": "^7.29.3",
		"html-webpack-plugin": "^5.5.0",
		"husky": "^7.0.4",
		"lint-staged": "^12.3.4",
		"node-sass": "^7.0.1",
		"postcss-loader": "^6.2.1",
		"prettier": "^2.5.1",
		"sass-loader": "^12.6.0",
		"style-loader": "^3.3.1",
